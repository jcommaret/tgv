{
  "name": "tgv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://jcommaret.github.io/tgv",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
